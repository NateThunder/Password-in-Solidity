pragma solidity ^0.8.30;

contract padlock{
    
    uint256 private pin;


    function password(uint256 _pin) public pure returns (string memory) {
        if (_pin == 1234){
            return "Password Correct";
        }else{
            return "Wrong password";

        }

    }

}
